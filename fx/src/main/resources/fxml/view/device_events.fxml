<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Button fx:id="goBackBtn" mnemonicParsing="false" text="Go back">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <Label styleClass="title" text="Scheduled alarms" />
      <TableView fx:id="table">
        <columns>
            <TableColumn fx:id="timeCol" minWidth="-Infinity" prefWidth="60.0" resizable="false" text="Time" />
            <TableColumn fx:id="soundCol" minWidth="-Infinity" prefWidth="60.0" resizable="false" text="Sound" />
            <TableColumn fx:id="volCol" minWidth="-Infinity" prefWidth="50.0" resizable="false" text="vol." />
          <TableColumn fx:id="snoozeCol" minWidth="-Infinity" prefWidth="60.0" resizable="false" text="Snooze" />
            <TableColumn fx:id="shutoffCol" minWidth="-Infinity" prefWidth="60.0" resizable="false" text="Shutoff" />
        </columns>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
      </TableView>
   </children>
   <stylesheets>
       <URL value="@../../css/Main.css" />
   </stylesheets>
</VBox>
