<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="5.0" stylesheets="@../../css/Style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="title" text="Reset password" />
      <Region prefHeight="22.0" prefWidth="200.0" />
      <Label prefHeight="32.0" prefWidth="158.0" styleClass="title" text="Please enter your email:" />
      <HBox alignment="CENTER">
         <children>
            <TextField fx:id="emailTxtField" alignment="CENTER" />
         </children>
      </HBox>
      <AnchorPane>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </AnchorPane>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <Button fx:id="goBackButton" minWidth="70.0" mnemonicParsing="false" text="Go back" />
            <Button fx:id="sendButton" minWidth="70.0" mnemonicParsing="false" text="Send" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
