<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" spacing="10.0" stylesheets="@../../css/Style.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Button fx:id="goBackBtn" minWidth="70.0" mnemonicParsing="false" text="Go back" />
         </children>
      </HBox><HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0">
         <children>
            <Label styleClass="title" text="Device name:" />
            <TextField fx:id="inputTxtField" />
         </children>
      </HBox>
      <Button fx:id="okBtn" minWidth="70.0" mnemonicParsing="false" text="Ok" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
